	public Interface() {
		
		super("Arbre Binaire");
		setDefaultCloseOperation(EXIT_ON_CLOSE);
		
		Container c = getContentPane();
		c.setLayout(new BorderLayout());
		
		JPanel panelAire = new JPanel();
		panelAire.setLayout(new BorderLayout(BorderLayout.NORTH));
		area = new JTextArea(10,20);
		JPanel.add(area);
		
		pack();
		setVisible(true);
		
	}

